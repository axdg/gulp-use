{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "describe": false,
    "it": false
  },
  "extends": "airbnb/base",
  "rules": {
    "no-var": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "object-shorthand": 0,
    "no-new": 1,
    "vars-on-top": 1,
    "new-cap": 1
  }
}
